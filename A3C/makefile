	python3 GA3C.py PATH=$(path) MODEL_SETTING=4 AGENTS=8 MODEL_NAME=$(MODEL_NAME) TRAINING_MIN_BATCH_SIZE=10
	python3 GA3C.py PATH=$(path) MODEL_SETTING=5 AGENTS=8 MODEL_NAME=$(MODEL_NAME) TRAINING_MIN_BATCH_SIZE=10
	python3 GA3C.py PATH=$(path) MODEL_SETTING=6 AGENTS=8 MODEL_NAME=$(MODEL_NAME) TRAINING_MIN_BATCH_SIZE=10
	python3 GA3C.py PATH=$(path) MODEL_SETTING=7 AGENTS=8 MODEL_NAME=$(MODEL_NAME) TRAINING_MIN_BATCH_SIZE=10
	python3 GA3C.py PATH=$(path) MODEL_SETTING=8 AGENTS=8 MODEL_NAME=$(MODEL_NAME) TRAINING_MIN_BATCH_SIZE=10
	python3 GA3C.py PATH=$(path) MODEL_SETTING=9 AGENTS=8 MODEL_NAME=$(MODEL_NAME) TRAINING_MIN_BATCH_SIZE=10
	python3 GA3C.py PATH=$(path) MODEL_SETTING=10 AGENTS=8 MODEL_NAME=$(MODEL_NAME) TRAINING_MIN_BATCH_SIZE=10
