res :
	python3 GA3C.py MODEL_NAME=$(MODEL_NAME) RESTORE=1
tsp :
	mkdir -p figs
	mkdir -p ini_files
	mkdir -p logs
	mkdir -p checkpoint
	python3 GA3C.py MODEL_SETTING=$(MODEL_SETTING) AGENTS=8 MODEL_NAME=$(MODEL_NAME)
clean :
	rm -rf ./logs/*
	rm -rf ./checkpoint/*
